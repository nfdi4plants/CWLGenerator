{
  "name": "cwlgenerator",
  "version": "0.0.5",
  "description": "A tool that generates .cwl files by asking a series of questions.",
  "main": "dist/writeCWL.js",
  "preferGlobal": true,
  "bin": {
    "cwlgenerator": "dist/writeCWL.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/writeCWL.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfdi4plants/CWLGenerator.git"
  },
  "pkg": {
    "assets": [ "node_modules/**/*", "src/*" ],
    "targets": [ "node12-macos-x64", "node10-win-x64", "node10-linux-x64" ]
  },
  "author": "nfdi4plants",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfdi4plants/CWLGenerator/issues"
  },
  "homepage": "https://github.com/nfdi4plants/CWLGenerator#readme",
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.11.12",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "cwl-ts-auto": "file:cwl-ts-auto",
    "inquirer": "^8.2.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.6",
    "uri-js": "^4.4.1",
    "uuid": "^8.3.2"
  },
  "files": [
    "/dist",
    "/cwl-ts-auto"
  ]
}
